{
    "name": "reioto/pimple-register",
    "type": "library",
    "description": "Loader for Pimple ServiceProviderInterface",
    "keywords": ["dependency injection", "container", "autoload"],
    "license": "MIT",
    "require": {
	"php": ">=5.3",
        "pimple/pimple": "^3.0"
    },
    "autoload": {
	"psr-4": {
	    "PimpleRegister\\": "src/"
	}
    },
    "require-dev": {
	"phpunit/phpunit": "^4.8"
    },
    "autoload-dev": {
	"psr-4": {
	    "Test\\": "tests/src"
	}
    }

}
